{
	"name": "obsidian-metrics-plugin",
	"version": "1.0.0",
	"description": "Obsidian plugin with Prometheus server and metrics API",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["obsidian", "prometheus", "metrics", "monitoring"],
	"author": "Daniel Grossmann-Kavanagh",
	"license": "MIT",
	"dependencies": {
		"prom-client": "^15.1.0"
	},
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.25.0",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
